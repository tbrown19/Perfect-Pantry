{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
      {{#text.headline}}
        {{fa-icon 'cutlery'}} Pantry
      {{/text.headline}}
    {{/title.text}}
    {{#card.actions class = "dashboard-card-buttons"}}
      {{#paper-button iconButton=true}}{{paper-icon "favorite"}}{{/paper-button}}
      {{#paper-button iconButton=true}}{{paper-icon "settings"}}{{/paper-button}}
    {{/card.actions}}
  {{/card.title}}
  {{paper-divider}}
  {{#card.content}}
    <ul>
      {{#each model.users as |user|}}
        {{#each user.purchasedList.purchasedListItems as |item|}}
          <li class="list-item-dash">{{item.name}} {{item.purchasedDate}}</li>
        {{/each}}
      {{/each}}
    </ul>
  {{/card.content}}
{{/paper-card}}
