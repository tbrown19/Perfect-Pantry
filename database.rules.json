{
   "rules":{
      ".read":"auth != null",
      ".write":"auth != null",
      "users":{
         ".indexOn":[
            "email"
         ]
      },
      "shoppingListItems":{
         "$shoppingListItem":{
            "name":{
               ".validate":"newData.val().length > 2"
            },
            ".validate":"newData.hasChildren(['name','shoppingList'])"
         }
      }
   }
}
